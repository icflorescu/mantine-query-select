{
  "name": "mantine-query-select",
  "version": "0.0.1",
  "description": "Mantine Select powered by TanStack Query",
  "homepage": "https://github.com/icflorescu/mantine-query-select#readme",
  "packageManager": "yarn@4.0.1",
  "license": "MIT",
  "author": "Ionut-Cristian Florescu <ionut.florescu@gmail.com>",
  "keywords": [
    "mantine",
    "select",
    "dropdown",
    "async",
    "query",
    "tanstack",
    "query",
    "tanstack-query",
    "extension",
    "react",
    "ui",
    "component"
  ],
  "main": "./dist/cjs/index.cjs",
  "module": "./dist/esm/index.mjs",
  "types": "./dist/types/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/types/index.d.mts",
        "default": "./dist/esm/index.mjs"
      },
      "require": {
        "types": "./dist/types/index.d.ts",
        "default": "./dist/cjs/index.cjs"
      }
    },
    "./styles.css": "./dist/styles.css",
    "./styles.layer.css": "./dist/styles.layer.css"
  },
  "repository": "https://github.com/icflorescu/mantine-query-select.git",
  "peerDependencies": {
    "@mantine/core": ">=8.0.0",
    "@mantine/hooks": ">=8.0.0",
    "react": "^18.x || ^19.x",
    "react-dom": "^18.x || ^19.x"
  },
  "bugs": "https://github.com/icflorescu/mantine-query-select/issues"
}
